@page
@model LibraryManagementSystem.Pages.Books.GetAllAuthorsModel
@{
}
<h1 style="color:deepskyblue"> List of All Authors</h1>
<table class="table">
    <thead>
        <tr>
            <th> S.N. </th>
             <th> Author Name </th>
            
        </tr>
    </thead>
    <tbody>
        
        @if (Model != null)
        {
            @foreach (var @item in Model.Authors)
            {
                <tr>
                    <td>  @item.AuthorId </td>
                     <td>  @item.Name </td>
                    
                    
                 </tr>
            }
        }
</table>
